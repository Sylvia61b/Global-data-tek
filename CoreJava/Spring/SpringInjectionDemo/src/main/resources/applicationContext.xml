<beans xmlns="http://www.springframework.org/schema/beans"
       xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
       xsi:schemaLocation="http://www.springframework.org/schema/beans
                           http://www.springframework.org/schema/beans/spring-beans.xsd">

    <!-- Define the dependency bean -->
    <bean id="dependencyService" class="com.example.DependencyService" />

    <!-- Define MyService bean with constructor injection -->
    <bean id="ConstructorInjectService" class="com.example.ConstructorInjectService">
        <constructor-arg name="name" value="Spring Project" />
        <constructor-arg name="dependency" ref="dependencyService" />
        <constructor-arg name="stringList">
            <list>
                <value>Item1</value>
                <value>Item2</value>
            </list>
        </constructor-arg>
        <constructor-arg name="integerList">
            <list>
                <value>1</value>
                <value>2</value>
                <value>3</value>
            </list>
        </constructor-arg>
        <constructor-arg name="stringSet">
            <set>
                <value>Set1</value>
                <value>Set2</value>
            </set>
        </constructor-arg>
        <constructor-arg name="stringMap">
            <map>
                <entry key="Key1" value="10" />
                <entry key="Key2" value="20" />
            </map>
        </constructor-arg>
        <constructor-arg name="intMap">
            <map>
                <entry key="1" value="Val1" />
                <entry key="2" value="Val2" />
            </map>
        </constructor-arg>
    </bean>

    <!-- Define Service bean with setter injection -->
    <bean id="SetterInjectService" class="com.example.SetterInjectService">
        <property name="name" value="Spring Project" />
        <property name="dependency" ref="dependencyService" />
        <property name="stringList">
            <list>
                <value>Item1</value>
                <value>Item2</value>
            </list>
        </property>
        <property name="integerList">
            <list>
                <value>1</value>
                <value>2</value>
                <value>3</value>
            </list>
        </property>
        <property name="stringSet">
            <set>
                <value>Set1</value>
                <value>Set2</value>
            </set>
        </property>
        <property name="stringMap">
            <map>
                <entry key="Key1" value="10" />
                <entry key="Key2" value="20" />
            </map>
        </property>
        <property name="intMap">
            <map>
                <entry key="1" value="Val1" />
                <entry key="2" value="Val2" />
            </map>
        </property>
    </bean>
</beans>
